        <script>
            function updateFormAction() {
                var language = window.location.pathname.split('/')[1]; // Extract language from the route
                var userInput = document.getElementById('word').value.replace(/\s+/g, '-'); 
                var encodedUserInput = encodeURIComponent(userInput); 
                var destinationURL = '/' + language + '/english-to-' + language + '-meaning-' + encodedUserInput;

                // Navigate to the desired URL
                window.location.href = destinationURL;

                // Prevent default form submission
                return false;
            }

            // Handle keydown event on the search bar
            document.getElementById('word').addEventListener('keydown', function (event) {
                if (event.key === 'Enter') {
                    if (document.getElementById('similarWordsList').innerHTML.includes('No words found')) {
                        event.preventDefault();
                    } else {
                        updateFormAction();
                    }
                }
            });



            function speakWord() {
                var wordToSpeak = "<%= word %>";
                var speechSynthesisUtterance = new SpeechSynthesisUtterance(wordToSpeak);
                var voices = window.speechSynthesis.getVoices();
                speechSynthesisUtterance.voice = voices[0];
                window.speechSynthesis.speak(speechSynthesisUtterance);
                var speakButton = document.getElementById("speakButton");
                speakButton.classList.add("speaking");
                speechSynthesisUtterance.onend = function () {
                    speakButton.classList.remove("speaking");
                };
            }
        </script>
