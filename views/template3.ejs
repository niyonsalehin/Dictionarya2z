     <% if ((synonyms && synonyms.length > 0) && (sentences && sentences.length > 0)) { %>
     <section class="sentence-examples">
        <div class="my-container">
            <%
                const uniqueSynonyms = new Set();
                if (synonyms && synonyms.length > 0) {
                    synonyms.forEach(synonym => {
                        const synonymArray = synonym.split(',');
                        synonymArray.forEach(uniqueSynonym => {
                            const trimmedSynonym = uniqueSynonym.trim();
                            if (!uniqueSynonyms.has(trimmedSynonym) && trimmedSynonym.toLowerCase() !== word.toLowerCase()) {
                                uniqueSynonyms.add(trimmedSynonym);
                            }
                        });
                    });
                }
            %>
            <% if (uniqueSynonyms.size > 0) { %>
            <h3 class="hadding text-center">Synonyms</h3>
            <ol>
                
                <% uniqueSynonyms.forEach(synonym => { %>
                <li><a href="/<%= language %>/english-to-<%= language %>-meaning-<%- synonym %>"  rel="dofollow" style="color:black;"><%- synonym %></a></li>
                <% }); %>
            </ol>
            <% } %>
            <% if (sentences && sentences.length > 0) { %>
            <h3 class="hadding text-center">Sentence Examples</h3>
            <% const uniqueSentences = new Set(); %>
            <ol>
                <% sentences.forEach((sentence, index) => { %>
                <% const formattedSentence = sentence.replace(new RegExp(word, 'gi'), '<strong>$&</strong>'); %>
                <% if (!uniqueSentences.has(formattedSentence)) { %>
                <li><%- formattedSentence %></li>
                 <% uniqueSentences.add(formattedSentence); %>
                <% } %>
                <% }); %>
            </ol>
            <% } %>
        </div>
     </section>
     <% } %>
