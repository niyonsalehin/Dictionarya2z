            let text = "";
            const uniqueValuesSet = new Set();
            searchedWordsArray.forEach(item => {
                uniqueValuesSet.add(item);
            });
            const uniqueValuesArray = Array.from(uniqueValuesSet);
            uniqueValuesArray.slice(-5).reverse().forEach(mysearchitem);
            document.getElementById("demo").innerHTML = text;                            
            function mysearchitem(item) {
              text += '<button><a href="/<%= language %>/english-to-<%= language %>-meaning-' + item + '" class="text-dark" rel="dofollow">' + item + '</a></button>'; 
            }
        </script>

        <script>
            $(document).ready(function () {
                const searchResultsContainer = $('#searchResultsContainer');
                $('#word').on('input', function () {
                    const inputValue = $(this).val();
                    if (inputValue.length > 0) {
                        searchResultsContainer.show();
                    } else {
                        searchResultsContainer.hide();
                    }


                    $.ajax({
                        url: '/<%= language %>/search/' + inputValue,
                        method: 'GET',
                        success: function (data) {
                            // Update the search results on the page
                            const similarWordsList = $('#similarWordsList');
                            similarWordsList.empty();
                            var language = window.location.pathname.split('/')[1];
                            if (data.length > 0) {
                                data.forEach(word => {
                                    const listItem = $('<li>').html('<a href="/' +
                                        language + '/english-to-' + language +
                                        '-meaning-' + encodeURIComponent(word) +
                                        '" rel="dofollow">' + word + '</a>');

                                    similarWordsList.append(listItem);
                                });
                            } else {
                                similarWordsList.append('<li>No words found</li>');
                            }
                        },
                        error: function (error) {
                            console.error(error);
                        }
                    });
                });
            });
        </script>

